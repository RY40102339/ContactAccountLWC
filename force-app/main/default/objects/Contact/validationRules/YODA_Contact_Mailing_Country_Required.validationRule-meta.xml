<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>YODA_Contact_Mailing_Country_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( CONTAINS($Profile.Name, &quot;YODA&quot;),NOT($Setup.Bypass_Settings__c.Bypass_Validation_Rules__c), ISBLANK(MailingCountry))</errorConditionFormula>
    <errorMessage>Mailing Country is required for this Contact.</errorMessage>
</ValidationRule>
